    "module": "commonjs",
    "target": "es6",
    "preserveConstEnums": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "removeComments": true,
    "resolveJsonModule": true
  },
  "include": [
    "puzzles/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
